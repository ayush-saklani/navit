{
  "name": "react-simple-qr-code-scanner",
  "version": "2.5.5",
  "description": "A super simple qr code scanner",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./validators": {
      "import": "./dist/src/validator/index.mjs",
      "types": "./dist/src/validator/index.d.ts"
    },
    "./validators/*": {
      "import": "./dist/src/validator/*.mjs",
      "types": "./dist/src/validator/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "qr codes",
    "qr-codes",
    "qrcodes",
    "qr code",
    "qr-code",
    "qrcode"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitchellston/qr-code-scanner"
  },
  "author": {
    "name": "Mitchellston"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mitchellston/qr-code-scanner/issues"
  },
  "devDependencies": {
    "@types/react": "^17.0.62",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsup": "^7.1.0",
    "typescript": "^4.9.5",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@zxing/library": "^0.19.3"
  }
}
